{% if quantity>0 %}

    <div id="cart" class="mnucart" style="background-color: yellow">
      <div class="header-shopping-num" id="cardcaunt">{{ quantity }}</div>
    </div>
{% else %}
    <div id="cart" class="mnucart" style="background-color: #005a87">
        <div class="header-shopping-num" id="cardcaunt">{{ quantity }}</div>
    </div>
    
{% endif %}
